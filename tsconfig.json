{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "lib": [
            "ES2022"
        ],
        "moduleResolution": "bundler",
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "types": [
            "@cloudflare/workers-types"
        ]
    },
    "include": [
        "functions/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}